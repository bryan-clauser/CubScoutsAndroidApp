<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:background="@color/Blue"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
      <LinearLayout android:layout_height="fill_parent" 
    			  android:id="@+id/homeLayout" 
    			  android:orientation="vertical" 
    			  android:layout_width="fill_parent" 
    			  android:layout_margin="@dimen/Fivedip">

        <Button 
        	android:layout_width="fill_parent" 
        	android:layout_height="wrap_content" 
       		android:text="@string/bobcat"
       		android:textSize="@dimen/TwentyFivedip"
       		android:height="@dimen/SeventyFivedip"
       		android:id="@+id/bobcats" 
     	>
        </Button>    			  
        <Button 
        	android:layout_width="fill_parent" 
        	android:layout_height="wrap_content" 
       		android:text="@string/tiger"
       		android:textSize="@dimen/TwentyFivedip"
       		android:height="@dimen/SeventyFivedip"
       		android:id="@+id/tiger" 
     	>
        </Button>
        <Button 
        	android:layout_width="fill_parent" 
        	android:layout_height="wrap_content" 
        	android:text="@string/wolf"
        	android:textSize="@dimen/TwentyFivedip"
        	android:height="@dimen/SeventyFivedip" 
        	android:id="@+id/wolf" 
        >
        </Button>
        <Button 
        	android:layout_width="fill_parent" 
        	android:layout_height="wrap_content" 
        	android:text="@string/bear" 
        	android:textSize="@dimen/TwentyFivedip"
        	android:height="@dimen/SeventyFivedip"
        	android:id="@+id/bear" 
        >
        </Button>
        
        <Button 
        	android:layout_width="fill_parent" 
        	android:layout_height="wrap_content" 
        	android:text="@string/weblos" 
        	android:textSize="@dimen/TwentyFivedip"
        	android:height="@dimen/SeventyFivedip"
        	android:id="@+id/weblos" 
        >
        </Button>
        
    </LinearLayout>
</LinearLayout>
